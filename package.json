{
  "name": "nates-react-component-library",
  "version": "1.3.0",
  "main": "dist/index.js",
  "private": true,
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "react-scripts": "4.0.3",
    "@babel/cli": "7.15.7",
    "@babel/preset-env": "7.15.8",
    "@babel/preset-react": "7.14.5",
    "rimraf": "3.0.2"
  },
  "scripts": {
    "prestart": "rimraf dist && rimraf node_modules/react && rimraf node_modules/react-dom",
    "start": "npm run watch",
    "prewatch": "npm link",
    "watch": "npx babel src/lib --watch --out-dir dist/ --copy-files",
    "prebuild": "rimraf dist",
    "build": "npx babel src/lib --out-dir dist/ --copy-files",
    "test": "react-scripts test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nnmcdonald/nates-react-component-library"
  },
  "keywords": [
    "react",
    "component"
  ],
  "author": "Nathaniel McDonald",
  "license": "ISC",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
